 imgcreate/__init__.py |    8 +++++---
 1 files changed, 5 insertions(+), 3 deletions(-)

diff --git a/imgcreate/__init__.py b/imgcreate/__init__.py
index bcfb40e..e89d388 100644
--- a/imgcreate/__init__.py
+++ b/imgcreate/__init__.py
@@ -21,7 +21,7 @@ from imgcreate.creator import *
 from imgcreate.yuminst import *
 from imgcreate.kickstart import *
 from imgcreate.fs import *
-from imgcreate.debug import *
+from imgcreate.appliance import *
 
 """A set of classes for building Fedora system images.
 
@@ -29,6 +29,7 @@ The following image creators are available:
   - ImageCreator - installs to a directory
   - LoopImageCreator - installs to an ext3 image
   - LiveImageCreator - installs to a bootable ISO
+  - ApplianceImageCreator - installs to one or more partitioned disk images
 
 Also exported are:
   - CreatorError - all exceptions throw are of this type
@@ -61,8 +62,9 @@ __all__ = (
     'ImageCreator',
     'LiveImageCreator',
     'LoopImageCreator',
+    'ApplianceImageCreator',
     'FSLABEL_MAXLEN',
     'read_kickstart',
-    'construct_name',
-    'setup_logging',
+    'construct_name'
 )
+
